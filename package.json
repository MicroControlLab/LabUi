{
  "name": "lab-gui",
  "version": "0.0.1",
  "description": "UI Framework for controling a lab",
  "keywords": ["react", "Ui", "typescript"],
  "main": "dist/lab-gui.umd.js",
  "module": "dist/lab-gui.es5.js",
  "typings": "dist/types/lab-gui.d.ts",
  "files": ["dist"],
  "author": "Sebastian Weigand <s.weigand.phy@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MicroControlLab/lab-gui"
  },
  "bugs": {
    "url": "https://github.com/MicroControlLab/lab-gui-websocket.git/issues"
  },
  "homepage": "https://github.com/MicroControlLab/lab-gui-websocket.git",
  "license": "Apache License 2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint -t codeFrame -c tslint.json 'src/**/*.{ts,tsx,js}' 'tests/**/*.{ts,tsx,js}'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "demo": "parcel demos/demo.html"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,tsx,js}": ["prettier --write", "git add"]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "coveragePathIgnorePatterns": ["/node_modules/", "/tests/"],
    "collectCoverageFrom": ["src/*.{js,ts,tsx}"]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^11.9.0",
    "@types/parcel-env": "0.0.0",
    "@types/plotly.js": "^1.38.12",
    "@types/rc-slider": "^8.6.0",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-plotly.js": "^2.2.2",
    "@types/react-redux": "^7.0.0",
    "@types/redux-logger": "^3.0.6",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.1",
    "i": "^0.3.6",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "lint-staged": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "npm": "^6.5.0",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "redux-devtools-extension": "^2.13.5",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^1.0.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.19.0",
    "semantic-release": "^15.9.16",
    "ts-jest": "^24.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typedoc": "^0.14.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "plotly.js": "^1.42.1",
    "rc-slider": "^8.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-plotly.js": "^2.2.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "lab-gui-websocket": "^2.0.0"
  }
}
